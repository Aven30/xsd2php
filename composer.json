{
  "name": "goetas-webservices/xsd2php",
  "description": "Convert XSD  (XML Schema) definitions into PHP classes",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "LGPL",
  "require": {
    "php": ">=5.5",
    "symfony/console": "^2.1|^3.0",
    "symfony/dependency-injection": "^2.1|^3.0",
    "symfony/config": "^2.1|^3.0",
    "symfony/yaml": "^2.1|^3.0",
    "goetas-webservices/xsd-reader": "~0.1.0",
    "goetas-webservices/wsdl-reader": "~0.1.0",
    "doctrine/inflector": "^1.0",
    "zendframework/zend-code": "~2.3",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "jms/serializer": "serializer-master-dev as 1.0",
    "goetas-webservices/xsd2php-runtime": "~0.2.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/goetas/serializer.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "minimum-stability":"dev",
  "bin": [
    "bin/xsd2php"
  ]
}
