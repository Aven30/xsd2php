{
  "name": "goetas-webservices/xsd2php",
  "description": "Convert XSD  (XML Schema) definitions into PHP classes",
  "type": "library",
  "authors": [
    {
      "name": "Asmir Mustafic"
    }
  ],
  "keywords": [
    "converter",
    "xml",
    "xsd",
    "php",
    "jms",
    "serializer"
  ],
  "license": "LGPL",
  "require": {
    "php": "^5.5||7.0",
    "symfony/console": "^2.1@stable|^3.0@stable",
    "symfony/dependency-injection": "^2.1@stable|^3.0@stable",
    "symfony/config": "^2.1@stable|^3.0@stable",
    "symfony/yaml": "^2.1@stable|^3.0@stable",
    "goetas-webservices/xsd-reader": "^0.1.0",
    "goetas-webservices/soap-reader": "^0.1.0",
    "doctrine/inflector": "^1.0@stable",
    "zendframework/zend-code": "~2.3@stable",
    "psr/log": "^1.0@stable"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8@stable|^5.0@stable",
    "jms/serializer": "serializer-master-dev as 1.0",
    "goetas-webservices/xsd2php-runtime": "^0.2.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/goetas/serializer.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },

  "prefer-stable": false,
  "bin": [
    "bin/xsd2php"
  ]
}
