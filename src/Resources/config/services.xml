<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="goetas.xsd2php.naming_convention.short" class="GoetasWebservices\Xsd\XsdToPhp\Naming\ShortNamingStrategy"/>
        <service id="goetas.xsd2php.naming_convention.long" class="GoetasWebservices\Xsd\XsdToPhp\Naming\LongNamingStrategy"/>

        <service abstract="true" id="goetas.xsd2php.naming_convention"/>

        <service id="goetas.xsd2php.converter.php" class="GoetasWebservices\Xsd\XsdToPhp\Php\PhpConverter">
            <argument id="goetas.xsd2php.naming_convention" type="service"/>
        </service>

        <service id="goetas.xsd2php.converter.jms_yaml" class="GoetasWebservices\Xsd\XsdToPhp\Jms\YamlConverter">
            <argument id="goetas.xsd2php.naming_convention" type="service"/>
        </service>

        <service id="goetas.xsd2php.converter.extend.php.soap" class="GoetasWebservices\Xsd\XsdToPhp\Php\PhpSoapConverter">
            <argument id="goetas.xsd2php.converter.php" type="service"/>
        </service>

        <service id="goetas.xsd2php.converter.extend.jms_yaml.soap" class="GoetasWebservices\Xsd\XsdToPhp\Jms\YamlSoapConverter">
            <argument id="goetas.xsd2php.converter.jms_yaml" type="service"/>
        </service>

        <service abstract="true" id="goetas.xsd2php.path_generator.php"/>
        <service abstract="true" id="goetas.xsd2php.path_generator.jms_yaml"/>
        <service id="goetas.xsd2php.path_generator.php.psr4" class="GoetasWebservices\Xsd\XsdToPhp\Php\PathGenerator\Psr4PathGenerator">
        </service>
        <service id="goetas.xsd2php.path_generator.jms_yaml.psr4" class="GoetasWebservices\Xsd\XsdToPhp\Jms\PathGenerator\Psr4PathGenerator">
        </service>

        <service id="goetas.xsd2php.writer.php" class="GoetasWebservices\Xsd\XsdToPhp\Writer\PHPWriter">
            <argument id="goetas.xsd2php.path_generator.php" type="service"/>
        </service>
        <service id="goetas.xsd2php.writer.jms_yaml" class="GoetasWebservices\Xsd\XsdToPhp\Writer\JMSWriter">
            <argument id="goetas.xsd2php.path_generator.jms_yaml" type="service"/>
        </service>

    </services>
</container>